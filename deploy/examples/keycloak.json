{
  "apiVersion": "aerogear.org/v1alpha1",
  "kind": "Keycloak",
  "metadata": {
    "name": "example"
  },
  "spec": {
    "version": "4.1.0",
    "adminCredentials": "",
    "realms": [
      {
        "id": "arealm",
        "realm": "arealm",
        "displayName": "arealm",
        "enabled": true,
        "users": [
          {
            "username": "admin",
            "enabled": true,
            "emailVerified": false,
            "firstName": "",
            "lastName": "",
            "email": "",
            "realmRoles": [
              "offline_access",
              "uma_authorization"
            ],
            "clientRoles": {
              "account": [
                "manage-account",
                "view-profile"
              ]
            }
          },
          {
            "username": "test",
            "enabled": true,
            "realmRoles": [
              "offline_access",
              "uma_authorization"
            ],
            "clientRoles": {
              "account": [
                "manage-account",
                "view-profile"
              ]
            }
          }
        ],
        "clients": [
          {
            "name": "mobile",
            "clientId": "mobile-test",
            "publicClient": true,
            "webOrigins": [
              "http://localhost:8100",
              "*"
            ],
            "redirectUris": [
              "http://localhost:*"
            ],
            "standardFlowEnabled": true,
            "enabled": true
          },
          {
            "name": "server",
            "clientId": "server-test",
            "bearerOnly": true,
            "baseURL": "https://keycloak.com",
            "standardFlowEnabled": true,
            "enabled": true
          }
        ],
        "identityProviders": [
          {
            "alias": "github",
            "providerId": "github",
            "enabled": true,
            "firstBrokerLoginFlowAlias": "first broker login",
            "config": {
              "clientId": "test",
              "clientSecret": "test",
              "useJwksUrl": "true",
              "disableUserInfo": "",
              "hideOnLoginPage": ""
            }
          }
        ]
      }
    ]
  }
}